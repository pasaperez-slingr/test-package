{
    "name": "s2keleton",
    "label": "Skeleton Package",
    "dependencies": [
        {
            "type": "service",
            "name": "http",
            "version": "v1.6.3",
            "instanceId": "shared"
        }
    ],
    "configuration": [
        {
            "name": "SKELETON_API_BASE_URL",
            "label": "Skeleton Api Url",
            "type": "label",
            "value": "https://example.com/"
        }
    ],
    "events": [
        {
            "label": "Webhook",
            "name": "webhook",
            "description": "Event triggered when there is a webhook coming from PandaDoc."
        }
    ],
    "metadata": [
        {
            "type": "script",
            "namespace": "api",
            "path": "/scripts/api.js"
        }
    ]
}
